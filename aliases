# ------
# alias
# ------
alias ...='cd ../../'
alias g='git'
alias ls='exa'
alias ll='exa -lFh'
alias la='exa -lFha'
alias df='df -h'
alias irb='pry'
alias grep='grep --color=auto'
alias history='history -i'
alias mkdir='mkdir -p'
alias tree='tree -CF -L 1'
alias dc='docker-compose'
alias duu='du -h -d 1'
