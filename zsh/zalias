# ------
# alias
# ------
alias ls='ls -p'
alias ll='ls -lpFh'
alias la='ls -lpFha'
alias df='df -h'
alias rm='rm -i'
alias irb='pry'
alias grep='grep --color=auto'
alias history='history -i'
alias timeadjust='sudo service ntpd start'
