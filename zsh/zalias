# ------
# alias
# ------
alias ll='ls -lpFh --color=auto'
alias la='ls -lpFha --color=auto'
alias df='df -h'
alias rm='rm -i'
alias irb='pry'
alias grep='grep --color=auto'
alias history='history -i'
alias timeadjust='sudo service ntpd start' # for linux VM working on VirtualBox

